package tp.kits3.demo;

import java.sql.ResultSet;
import java.sql.SQLException;

public abstract class tagAbstract extends tagServlet{
	String tname;
	int tid;
	@Override
	protected void resolver(ResultSet rs) {
		// TODO Auto-generated method stub
		try {
			tid = rs.getInt("tagid");
		} catch (SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}

		try {
			tname = rs.getString("tgname");
		} catch (SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		System.out.println(tid+":"+tname);
	}
	
}
